<?xml version='1.0' encoding='utf-8'?>
<widget id="com.handywit.MagpieDemo" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <preference name="AndroidLaunchMode" value="singleTop" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <name>MagpieDemo</name>
    <description>
        Magpie App that allow Cocos2d-X game to call Cordova/PhoneGap Plugins.
    </description>
    <author email="handywit@gmail.com" href="https://github.com/handywit">
        HandyWit Magpie Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <feature name="AdMob">
        <param name="android-package" value="com.google.cordova.admob.AdMob" />
    </feature>
</widget>
